{
  "auto_sync": {
    "enabled": true,
    "schedule": [
      "07:00",
      "12:00",
      "16:00",
      "18:00"
    ],
    "days_back": 90
  },
  "manual_sync": {
    "default_days": 90,
    "max_days": 365,
    "min_days": 1
  },
  "performance": {
    "chunk_days": 7,
    "batch_size": 1000,
    "parallel_chunks": 2,
    "retry_count": 3
  },
  "query_cache": {
    "enabled": true,
    "ttl_minutes": 60,
    "fallback_on_stale": true
  },
  "memory_cache": {
    "enabled": true,
    "max_size": 2000,
    "ttl_seconds": 1800,
    "warm_on_startup": true,
    "warm_count": 100
  }
}
